.plyr.plyr--full-ui.plyr--video {
  color: $body_font_color;
  background-color: transparent;

  .plyr__poster,
  .plyr__video-wrapper {
    background-color: transparent;
  }

  &:fullscreen .plyr__video-wrapper &:fullscreen .plyr__poster {
    background-color: $black;
  }

  &:-webkit-full-screen .plyr__video-wrapper,
  &:-webkit-full-screen .plyr__poster {
    background-color: $black;
  }

  &:-moz-full-screen .plyr__video-wrapper,
  &:-moz-full-screen .plyr__poster {
    background-color: $black;
  }

  &:-ms-fullscreen .plyr__video-wrapper,
  &:-ms-fullscreen .plyr__poster {
    background-color: $black;
  }

  .plyr--fullscreen-fallback .plyr__video-wrapper,
  .plyr--fullscreen-fallback .plyr__poster {
    background-color: $black;
  }

  .plyr__control.plyr__control--overlaid {
    background-color: $white;
    border-color: rgba($body_font_color, 0.05);

    &.plyr__tab-focus,
    &:hover {
      color: rgba($body_font_color, 0.55);
    }
  }

  .plyr__controls {
    background-color: $white;
    border-color: rgba($body_font_color, 0.05);
  }

  .plyr__progress {
    input[type='range'] {
      &::-moz-range-thumb {
        box-shadow: 2px 0 0 0 $white;
      }

      &::-ms-thumb {
        box-shadow: 2px 0 0 0 $white;
      }

      &::-webkit-slider-thumb {
        box-shadow: 2px 0 0 0 $white;
      }

      &::-webkit-slider-runnable-track {
        background-image: linear-gradient(
          to right,
          currentColor var(--value, 0),
          rgba($body_font_color, 0.6) var(--value, 0)
        );
      }

      &::-moz-range-track {
        background-color: rgba($body_font_color, 0.6);
      }

      &::-ms-fill-upper {
        background-color: rgba($body_font_color, 0.6);
      }

      &.plyr__tab-focus {
        &::-webkit-slider-runnable-track {
          box-shadow: 0 0 0 4px rgba($body_font_color, 0.25);
        }

        &::-moz-range-track {
          box-shadow: 0 0 0 4px rgba($body_font_color, 0.25);
        }

        &::-ms-track {
          box-shadow: 0 0 0 4px rgba($body_font_color, 0.25);
        }
      }

      &:active {
        &::-moz-range-thumb {
          box-shadow: 0 0 0 3px rgba($body_font_color, 0.25);
        }

        &::-ms-thumb {
          box-shadow: 0 0 0 3px rgba($body_font_color, 0.25);
        }

        &::-webkit-slider-thumb {
          box-shadow: 0 0 0 3px rgba($body_font_color, 0.25);
        }
      }
    }

    .plyr__tooltip {
      background-color: $body_font_color;
      color: $white;

      &::before {
        border-top-color: $body_font_color;
      }
    }
  }

  &.plyr--loading .plyr__progress__buffer {
    background-image: linear-gradient(
      -45deg,
      rgba($body_font_color, 0.6) 25%,
      transparent 25%,
      transparent 50%,
      rgba($body_font_color, 0.6) 50%,
      rgba($body_font_color, 0.6) 75%,
      transparent 75%,
      transparent
    );
  }

  .plyr__volume {
    input[type='range'] {
      color: $white;

      &::-moz-range-thumb {
        box-shadow: 2px 0 0 0 $body_font_color;
      }

      &::-ms-thumb {
        box-shadow: 2px 0 0 0 $body_font_color;
      }

      &::-webkit-slider-thumb {
        box-shadow: 2px 0 0 0 $body_font_color;
      }

      &::-webkit-slider-runnable-track {
        background-image: linear-gradient(
          to right,
          currentColor var(--value, 0),
          rgba($white, 0.6) var(--value, 0)
        );
      }

      &::-moz-range-track,
      &::-ms-fill-upper {
        background-color: rgba($white, 0.6);
      }

      &.plyr__tab-focus {
        &::-webkit-slider-runnable-track {
          box-shadow: 0 0 0 4px rgba($white, 0.25);
        }

        &::-moz-range-track {
          box-shadow: 0 0 0 4px rgba($white, 0.25);
        }

        &::-ms-track {
          box-shadow: 0 0 0 4px rgba($white, 0.25);
        }
      }

      &:active {
        &::-moz-range-thumb {
          box-shadow: 0 0 0 3px rgba($white, 0.25);
        }

        &::-ms-thumb {
          box-shadow: 0 0 0 3px rgba($white, 0.25);
        }

        &::-webkit-slider-thumb {
          box-shadow: 0 0 0 3px rgba($white, 0.25);
        }
      }
    }
  }
}


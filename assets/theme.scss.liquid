    }    
    
  }
  
  .easyzoom-flyout {
    position:absolute;
    z-index: 1000;
    background: $white;

    img {
      -ms-touch-action: none;
      touch-action: none;
    }
    
  }

  &.easyzoom--overlay .easyzoom-flyout {
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }

  &.easyzoom--adjacent .easyzoom-flyout {
    top: 0;
    left: 100%;
    width: 100%;

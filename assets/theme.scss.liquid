.one,
.two,
.two-half,
.three,
.four,
.five,
.six,
.seven,
.eight,
.nine,
.ten,
.eleven,
.twelve {
  float:left;
  font-size:$small;
  padding:0 $master_spacing / 2;		
  @include border-box();
  
  &.negative-left {
    padding-left:0;
  }
  
  &.negative-right {
    padding-right:0;
  }
  
  &.negative-left,
  &.negative-right {
    
    &.positive {
      
      @media #{$mobile} {
        padding-left:$master_spacing / 2;
        padding-right:$master_spacing / 2;
      }
      
    }
    
  }
  
  .table-center & {
    
    &.hidden {
      display:none;
    }
    
    &.top {
      vertical-align:top;
    }
    
    &.no-height {
      height:auto;
    } 

    &.brands {
      height:auto;
    }
    
    @media #{$mobile} {
      height:auto;

      &.brands,
      &.orders {
        width:50%;
      }
      
      .collection-products & {
        height: auto;
      }

    }
    
  }

  .table-center.product-variants & {
    vertical-align:top;
  }

  &.swiper-product {
    padding:0;

    @media #{$mobile} {

      .product {
        width:50%;
      }

    }

  }
  
  &.grid {
    @media #{$mobile} {
      padding-left:0;
      padding-right:0;
    }
  }

  &.margin-bottom {
    margin-bottom:$master_spacing / 2;
  }
  
  &.margin-top {
    margin-top:$master_spacing / 2;
  }
  
  &.margin-top-bottom {
    margin-bottom:$master_spacing / 2;
    margin-top:$master_spacing / 2;
  }
    
  @media #{$tablet} {

    .collection-products &,
    .cart-table &{
      width:100%;
    }

  }

  @media #{$mobile} {
    
    
    &:not(.mobile) {
      width:100%;
    }
    
    .purchase-section & {
      width:100%;
    }

    footer &:not(.brands) {
      padding:0 $master_spacing * 3;
    }
    
  }
  
}

.hidden {
  display:none;
  @include transition();
}

.left {
  float:left;
}

.right {
  float:right; 
}

.center {
  display:block;
  float:none;
  margin:0 auto; 
  text-align: center;
  
  &.center-table {
    display:table;
  }
  
  &.swiper-wrapper {
    display:block;
    height:auto;
  }
  
  @media #{$tablet} {

    &.swiper-wrapper {
      @include flex;
      height:100%;
    }

    .swiper-slide {
      height:auto;
    }

  }
  
}

.flex-wrap {
  @include flex-wrap;
  
  @media #{$mobile} {
    -webkit-flex-wrap: nowrap;
    -ms-flex-wrap: nowrap;
    flex-wrap: nowrap;
  }

}

.slide-center {
  float:none;
  margin: 0 auto;  
}

.table {
  display:table;
  height:100%;
  width:100%;
  
  &.left-width {
    width: auto;
    
    .mobile-only & {
      width: 100%;  
    }
    
  }

  &.icon-text-table {
    font-size:0;
    width: auto;

    .full-width & {
      width:100%;
    }
    
    .icon-text {
      @include border-box();
      display:inline-block;
      margin-bottom:$master_spacing / 2;
      padding:0 $master_spacing * 0.75;
      width:auto;

      .img {
        padding-right:$master_spacing / 2;

        img {
          margin:0;
          max-height:30px;
          max-width:30px;
        }

      }
      
    }

    @media #{$mobile} {
      margin: 0 auto;
      text-align:center;
    }

  }

}

.table-center {
  font-size:0;
}

.table-cell {
  display:table-cell;
  height:100%;
  vertical-align: middle;

  > div {
    padding-top:$master_spacing;
    
    &.no-height.image {
      padding-top:0;
      
      img,
      .placeholder-svg,
      .lazypadding div {
        margin-bottom:0;
      }
      
    }

    &.no-pad {
      padding-top:0;
    }
    
  }
  
  &.bottom {
    vertical-align: bottom;
  }
  
  &.top {
    vertical-align: top;
  }
  
}

.absolute {
  height: 100%;
  left:0;
  position:absolute;
  top:0;
  width: 100%;
}

.relative {
  position:relative;
  
  &.media-viewer {
    padding-top: 100%;
    margin: 0px;
  }
  
}

.clearfix {
  zoom:1;
  
  &:before,&:after {
    content:'';
    display:table;
  }

  &:after {
    clear:both;
  }
}

.background-white {
  background:$white;
  padding-top:$master_spacing;
  position:relative;
  
  &.needed {
    background:none;
    padding-top:0;

    .product-img,
    .no-img {
      background:$white;
      @include border-box();
      padding:$master_spacing;
    }

  }
  
}

.background-colored {

  &.color-primary {
    background:$section_color_primary;
  }
  
  &.color-secondary {
    background:$section_color_secondary;
  }
  
  &.colored-bottom {
    padding-bottom:$master_spacing * 3;
    
    @media #{$mobile} {
      padding-bottom:$master_spacing * 2;
    }
  
  }
  
  &.colored-top {
    padding-top:$master_spacing * 3;
    
    @media #{$mobile} {
      padding-top:$master_spacing * 2;
    }

  }
  
  &.colored-top-bottom {
    padding-bottom:$master_spacing * 3;
    padding-top:$master_spacing * 3;
    
    @media #{$mobile} {
      padding-top:$master_spacing * 2;
      padding-bottom:$master_spacing * 2;
    }

  }
  
}

.background-cover {
  @include background-cover();
}

.background-contain {
  @include background-contain();
}

.lazyload,
.lazyloading {
  position:relative;
  @include opacity-25();
  @include transition();

  &.background-cover {
    
    @include opacity-100();
    
    &:before {
      background:$white;
      content:'';
      height:100%;
      left:0;
      @include opacity-75();
      position:absolute;
      top:0;
      width:100%;
    }
    
  }
  
}

.lazyloaded {
  @include fast-transition();
}


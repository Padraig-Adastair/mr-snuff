        }

        &.active a:after {
          color:$button_color;
          content:$square-solid;
          @include opacity-100();
          text-decoration:none;
        }
        
        &.active a.hover:after,
        &.active a:hover:after,
        &.active a:focus:after {
          content:$x-only;
        }

        &.disabled span {
          @include opacity-50();
          padding-bottom:4px;
          padding-top:4px;

          &:after {
            content:$square-crossed;
          }

        }

      }


.two-half {
  width:20%;

  @media #{$tablet} {

    .purchase-section & {
      width:33.33%
    }

  }
  
}						/* 250px */
.three {width:25%;}		/* 300px */
.four {width:33.33%;}	/* 400px */
.five {width:41.66%;}	/* 500px */

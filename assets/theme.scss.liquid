.swiper-container {
  margin:-$master_spacing / 2 auto 0;
  position:relative;
  overflow:hidden;
  list-style:none;
  padding:$master_spacing / 2 0 0;
  -webkit-transform: translate3d(0,0,0);
  transform: translate3d(0,0,0);
  z-index:1;  
  
  &.negative-left {
    margin-left:-$master_spacing / 2;
  }
  
  &.negative-right {
    margin-right:-$master_spacing / 2;
  }
  
  &.plus-slider,
  &.plus-grid {
    
    &:before {  
      content:'';
      @include loader();
    }

    .swiper-wrapper {
      @include opacity-0();
      @include transition(); 
    }
    
  }

  &.plus-grid,
  &.swiper-container-horizontal {
    
    &:before { 
      display:none;
    }
    
    .swiper-wrapper{
      @include opacity-100(); 
    }
    
  }

  @media #{$mobile} {
    
    &.plus-slider-grid {

      &:before {  
        content:'';
        @include loader();
      }

      .swiper-wrapper {
        @include opacity-0();
        @include transition(); 
      }

    }

    &.plus-grid,
    &.swiper-container-horizontal {

      &:before { 
        display:none;
      }

      .swiper-wrapper{
        @include opacity-100(); 
      }

    }
    
    &.plus-slider,
    &.plus-slider-grid,
    &.plus-grid {  
      
      &:before { 
        display:block;
      }
      
      .swiper-wrapper {
        @include opacity-0(); 
      }

      &.swiper-container-horizontal {
        
        &:before { 
          display:none;
        }
        
        > div {
          @include opacity-100(); 
        }
        
      }

      .product {
        width:50%;
      }

    }
    
  }

}

